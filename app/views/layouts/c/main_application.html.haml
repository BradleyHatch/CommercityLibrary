!!!
%html
  %head
    = meta_tags
    = stylesheet_link_tag 'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    %meta{ content: 'width=device-width, initial-scale=1', name: 'viewport' }
    %link{ href: '/fav.png', rel: 'shortcut icon', type: 'image/png' }
    
    = render 'c/main_application/canonical'
    = render 'c/main_application/front_head_tags'
    
  %body.front
    = render 'c/main_application/application_bar'
    = render 'c/main_application/yield'
